{
    "name": "@avanio/jwt-util",
    "version": "0.1.1",
    "description": "NodeJS JWT utils",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts --clean",
        "prepublishOnly": "npm run build",
        "test": "vitest test --run --no-isolate --coverage",
        "coverage": "vitest test --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
        "lint": "biome check",
        "validate": "tsc --noEmit"
    },
    "files": [
        "dist"
    ],
    "repository": "github:mharj/jwt-util",
    "keywords": [
        "jwt",
        "nodejs"
    ],
    "author": "mharj",
    "license": "LGPL-2.0-only",
    "bugs": {
        "url": "https://github.com/mharj/jwt-util/issues"
    },
    "homepage": "https://github.com/mharj/jwt-util#readme",
    "devDependencies": {
        "@avanio/expire-cache": "^0.6.9",
        "@avanio/logger-like": "^0.2.12",
        "@azure/msal-node": "^5.0.3",
        "@biomejs/biome": "2.3.13",
        "@luolapeikko/auth-header": "^0.0.1",
        "@luolapeikko/cache-types": "^0.0.9",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^22.19.7",
        "@vitest/coverage-v8": "^4.0.18",
        "c8": "^10.1.3",
        "dotenv": "^17.2.3",
        "google-auth-library": "^10.5.0",
        "jsonwebtoken": "^9.0.3",
        "source-map-support": "^0.5.21",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "vitest": "^4.0.18",
        "zod": "^4.3.6"
    },
    "peerDependencies": {
        "@avanio/expire-cache": ">= 0.6.3",
        "@avanio/logger-like": ">= 0.0.1",
        "@luolapeikko/auth-header": ">= 0.0.1",
        "@luolapeikko/cache-types": "^0.0",
        "@types/jsonwebtoken": "^9.0",
        "jsonwebtoken": "^9.0"
    },
    "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
